{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    
        "limits": {
          "maxRequestBodySize": "100mb"
        },
      
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}
